<body>
    <h1>
        <%= roomId %>
    </h1>
    <div id='stars'></div>
    <div id='stars2'></div>
    <div id='stars3'></div>
    <div class="container">
        <div id="video"></div>
    </div>
    <script>
        const socket = io("/");
        socket.emit("join-room", ROOM_ID, USER_ID);
        console.log(ROOM_ID);

        socket.on('user-connected', (userId) => {
            console.log(`User ${userId} has connected to room ${ROOM_ID}`);
        });

        const videoDiv = document.getElementById("video");
        const videoEle = document.createElement('video');
        videoEle.muted = true;

        navigator.mediaDevices.getUserMedia({
            video: true,
            audio: true
        }).then(stream => {
            videoEle.srcObject = stream;
            videoEle.addEventListener('loadedmetadata', () => {
                videoEle.play();
            })
            videoDiv.append(videoEle);
        });

    </script>
